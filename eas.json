{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      },
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      },
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      },
      "ios": {
        "buildConfiguration": "Release",
        "resourceClass": "m-medium"
      },
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_APP_ENV": "production",
        "EXPO_PUBLIC_SUPABASE_URL": "$EXPO_PUBLIC_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "$EXPO_PUBLIC_SUPABASE_ANON_KEY",
        "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "$EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID",
        "EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID": "$EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID",
        "EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID": "$EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID",
        "EXPO_PUBLIC_HUGGINGFACE_TOKEN": "$EXPO_PUBLIC_HUGGINGFACE_TOKEN",
        "EXPO_PUBLIC_OPENAI_API_KEY": "$EXPO_PUBLIC_OPENAI_API_KEY",
        "EXPO_PUBLIC_USE_AI_PROXY": "true",
        "EXPO_PUBLIC_CLOUDINARY_CLOUD_NAME": "$EXPO_PUBLIC_CLOUDINARY_CLOUD_NAME",
        "EXPO_PUBLIC_CLOUDINARY_UPLOAD_PRESET": "$EXPO_PUBLIC_CLOUDINARY_UPLOAD_PRESET",
        "EXPO_PUBLIC_SENTRY_DSN": "$EXPO_PUBLIC_SENTRY_DSN",
        "EXPO_PUBLIC_WEATHER_API_KEY": "$EXPO_PUBLIC_WEATHER_API_KEY",
        "EXPO_PUBLIC_CDN_URL": "$EXPO_PUBLIC_CDN_URL",
        "EXPO_PUBLIC_ANALYTICS_ENABLED": "true",
        "EXPO_PUBLIC_CRASHLYTICS_ENABLED": "true"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "$APPLE_ID",
        "ascAppId": "$ASC_APP_ID",
        "appleTeamId": "$APPLE_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "./keys/google-play-service-account.json",
        "track": "internal"
      }
    }
  }
}
