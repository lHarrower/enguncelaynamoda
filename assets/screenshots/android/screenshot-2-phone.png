<svg width="360" height="640" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FAF9F6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#F5F5DC;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="360" height="640" fill="url(#bg)"/>
  
  <!-- Status Bar -->
  <rect x="0" y="0" width="360" height="24" fill="#000" opacity="0.1"/>
  <text x="20" y="18" font-family="Roboto, Arial" font-size="12" font-weight="500" fill="#000">9:41</text>
  <text x="320" y="18" font-family="Roboto, Arial" font-size="12" font-weight="500" fill="#000">100%</text>
  
  <!-- App Bar -->
  <rect x="0" y="24" width="360" height="56" fill="#FFFFFF" stroke="#E0E0E0" stroke-width="1"/>
  <text x="180" y="58" font-family="Roboto, Arial" font-size="20" font-weight="500" fill="#212121" text-anchor="middle">Sustainability Dashboard</text>
  
  <!-- Carbon Footprint Card -->
  <rect x="16" y="96" width="328" height="120" rx="12" fill="#FFFFFF" stroke="#E0E0E0" stroke-width="1"/>
  <text x="32" y="120" font-family="Roboto, Arial" font-size="16" font-weight="500" fill="#212121">Your Carbon Footprint</text>
  <text x="32" y="140" font-family="Roboto, Arial" font-size="12" fill="#757575">This month</text>
  
  <circle cx="280" cy="146" r="30" fill="none" stroke="#E0E0E0" stroke-width="4"/>
  <circle cx="280" cy="146" r="30" fill="none" stroke="#4CAF50" stroke-width="4" stroke-dasharray="113" stroke-dashoffset="28" transform="rotate(-90 280 146)"/>
  <text x="280" y="152" font-family="Roboto, Arial" font-size="14" font-weight="500" fill="#4CAF50" text-anchor="middle">75%</text>
  
  <text x="32" y="170" font-family="Roboto, Arial" font-size="24" font-weight="500" fill="#4CAF50">2.1 kg CO₂</text>
  <text x="32" y="190" font-family="Roboto, Arial" font-size="12" fill="#757575">25% less than last month 🌱</text>
  
  <!-- Achievements -->
  <text x="32" y="240" font-family="Roboto, Arial" font-size="18" font-weight="500" fill="#212121">Recent Achievements</text>
  
  <!-- Achievement 1 -->
  <rect x="16" y="256" width="328" height="56" rx="8" fill="#FFFFFF" stroke="#E0E0E0" stroke-width="1"/>
  <circle cx="48" cy="284" r="16" fill="#FF9800"/>
  <text x="48" y="290" font-family="Roboto, Arial" font-size="12" font-weight="500" fill="#FFFFFF" text-anchor="middle">🏆</text>
  <text x="80" y="280" font-family="Roboto, Arial" font-size="14" font-weight="500" fill="#212121">Eco Warrior</text>
  <text x="80" y="296" font-family="Roboto, Arial" font-size="12" fill="#757575">Wore sustainable outfits for 7 days straight</text>
  
  <!-- Achievement 2 -->
  <rect x="16" y="320" width="328" height="56" rx="8" fill="#FFFFFF" stroke="#E0E0E0" stroke-width="1"/>
  <circle cx="48" cy="348" r="16" fill="#4CAF50"/>
  <text x="48" y="354" font-family="Roboto, Arial" font-size="12" font-weight="500" fill="#FFFFFF" text-anchor="middle">♻️</text>
  <text x="80" y="344" font-family="Roboto, Arial" font-size="14" font-weight="500" fill="#212121">Wardrobe Optimizer</text>
  <text x="80" y="360" font-family="Roboto, Arial" font-size="12" fill="#757575">Increased wardrobe utilization to 89%</text>
  
  <!-- Sustainability Tips -->
  <text x="32" y="400" font-family="Roboto, Arial" font-size="18" font-weight="500" fill="#212121">Today's Eco Tip</text>
  
  <rect x="16" y="416" width="328" height="80" rx="8" fill="#E8F5E8" stroke="#4CAF50" stroke-width="1"/>
  <text x="32" y="440" font-family="Roboto, Arial" font-size="14" font-weight="500" fill="#2E7D32">💡 Smart Styling Tip</text>
  <text x="32" y="460" font-family="Roboto, Arial" font-size="12" fill="#2E7D32">Try wearing your blue jeans with different tops</text>
  <text x="32" y="475" font-family="Roboto, Arial" font-size="12" fill="#2E7D32">this week to maximize their potential!</text>
  
  <!-- Action Buttons -->
  <rect x="16" y="516" width="160" height="40" rx="20" fill="#4CAF50"/>
  <text x="96" y="540" font-family="Roboto, Arial" font-size="14" font-weight="500" fill="#FFFFFF" text-anchor="middle">View Report</text>
  
  <rect x="184" y="516" width="160" height="40" rx="20" fill="#FFFFFF" stroke="#4CAF50" stroke-width="1"/>
  <text x="264" y="540" font-family="Roboto, Arial" font-size="14" font-weight="500" fill="#4CAF50" text-anchor="middle">Share Progress</text>
  
  <!-- Bottom Navigation -->
  <rect x="0" y="580" width="360" height="60" fill="#FFFFFF" stroke="#E0E0E0" stroke-width="1"/>
  <text x="72" y="615" font-family="Roboto, Arial" font-size="10" font-weight="400" fill="#757575" text-anchor="middle">Home</text>
  <text x="180" y="615" font-family="Roboto, Arial" font-size="10" font-weight="500" fill="#6200EA" text-anchor="middle">Sustainability</text>
  <text x="288" y="615" font-family="Roboto, Arial" font-size="10" font-weight="400" fill="#757575" text-anchor="middle">Profile</text>
</svg>