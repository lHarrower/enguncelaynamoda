{
  "name": "aynamoda",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.test.json --noEmit",
    "lint": "eslint -c .eslintrc.cjs . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint -c .eslintrc.cjs . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml,css,scss}\" --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --runInBand",
    "secrets:check": "node scripts/validate-secrets.js",
    "prod:validate": "node scripts/validate-production-env.js",
    "prod:build": "npm run prod:validate && eas build --profile production --platform all",
    "prod:submit": "eas submit --profile production --platform all",
    "prod:deploy": "npm run prod:build && npm run prod:submit",
    "ai:smoke:or": "node scripts/ai-proxy-smoke.js or",
    "ai:smoke:vision": "node scripts/ai-proxy-smoke.js vision",
    "depcheck": "depcheck --skip-missing=true",
    "deadcode": "ts-prune -p tsconfig.app.json",
    "circulars": "madge src --extensions ts,tsx --circular",
    "audit:quick": "npm audit --omit=dev || exit 0",
    "doctor": "npx @expo/doctor@latest || exit 0",
    "audit:all": "npm run typecheck && npm run lint && npm run test:ci && npm run deadcode && npm run circulars && npm run depcheck && npm run audit:quick && npm run doctor",
    "audit:static": "powershell -ExecutionPolicy Bypass -File ./scripts/audit/run-static.ps1 -FailOnGate",
    "audit:coverage": "powershell -ExecutionPolicy Bypass -File ./scripts/audit/run-coverage.ps1",
    "audit:mutation": "powershell -ExecutionPolicy Bypass -File scripts/audit/run-mutation.ps1",
    "test:mutation": "stryker run",
    "test:mutation:focused": "stryker run --mutate src/services/**/*.ts,src/hooks/**/*.ts,src/utils/**/*.ts",
    "audit:secrets": "powershell -ExecutionPolicy Bypass -File ./scripts/audit/run-secrets.ps1 -FailOnGate",
    "bundle:analyze": "npx react-native-bundle-visualizer",
    "bundle:optimize": "npm run bundle:analyze && echo 'Review bundle-analysis/SUMMARY.md for optimization recommendations'",
    "audit:deps": "powershell -ExecutionPolicy Bypass -File ./scripts/audit/run-deps.ps1 -FailOnGate",
    "audit:rls": "npx ts-node scripts/audit/run-rls-tests.ts && npx ts-node scripts/audit/aggregate.ts",
    "audit:perf": "tsx scripts/audit/run-perf-placeholder.ts",
    "audit:a11y": "npx ts-node scripts/audit/run-a11y-placeholder.ts && npx ts-node scripts/audit/aggregate.ts",
    "audit:perf:parse": "node scripts/audit/parse-perf.js",
    "audit:perf:lighthouse": "npx @lhci/cli autorun",
    "audit:perf:monitor": "npm run audit:perf && npm run audit:perf:lighthouse",
    "audit:a11y:scan": "npx ts-node scripts/audit/a11y-scan.ts",
    "audit:pr": "npm run audit:static && npm run audit:coverage && npm run audit:perf:parse && npm run audit:a11y:scan && ts-node scripts/audit/aggregate.ts && npm run report:update",
    "audit:pdf": "npx ts-node scripts/audit/generate-pdf-report.ts",
    "audit:full": "npm run audit:static && npm run audit:coverage && npm run audit:mutation && npm run audit:secrets && npm run audit:deps && npm run audit:rls && npm run audit:perf:parse && npm run audit:a11y:scan && ts-node scripts/audit/aggregate.ts",
    "audit:badges": "ts-node scripts/audit/update-readme-badges.ts",
    "lint:gate": "eslint -c .eslintrc.cjs . --ext .ts,.tsx --max-warnings=800",
    "ci:gates": "node scripts/check-ci-gates.js",
    "bundle:size": "expo export --platform web && du -sh dist",
    "report:update": "node --no-warnings --no-deprecation --loader ts-node/esm scripts/audit/update-gercek-rapor.mts"
  },
  "dependencies": {
    "@expo/config-types": "^53.0.5",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.4.5",
    "@sentry/react-native": "~6.14.0",
    "@supabase/supabase-js": "^2.50.0",
    "expo": "53.0.22",
    "expo-apple-authentication": "~7.2.4",
    "expo-application": "^6.1.5",
    "expo-auth-session": "^6.2.1",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.4",
    "expo-device": "^7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "^14.1.4",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.6",
    "expo-location": "~18.1.6",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "^2.2.3",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "i18n-js": "^4.5.1",
    "nativewind": "^4.1.23",
    "openai": "^5.11.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-fs": "^2.20.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "tailwindcss": "^3.4.17",
    "victory-native": "^36.9.2",
    "zod": "3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo-google-fonts/playfair-display": "^0.4.1",
    "@shopify/restyle": "^2.4.5",
    "@stryker-mutator/core": "^9.0.1",
    "@stryker-mutator/jest-runner": "^9.0.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "babel-jest": "^30.0.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "depcheck": "^1.4.7",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "~29.7.0",
    "jest-axe": "^10.0.0",
    "madge": "^8.0.0",
    "prettier": "^3.6.2",
    "puppeteer": "^22.15.0",
    "react-native-bundle-visualizer": "^3.1.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-test-renderer": "^19.0.0",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.4",
    "typescript": "~5.8.3"
  },
  "private": true
}
